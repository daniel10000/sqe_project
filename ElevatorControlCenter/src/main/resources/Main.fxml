<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.cell.*?> 
<?import javafx.collections.*?> 
<?import fxmltableview.*?> 
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import com.sun.javafx.scene.control.skin.TableColumnHeader?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<GridPane alignment="CENTER" hgap="10.0" vgap="10.0"
	xmlns:fx="http://javafx.com/fxml" fx:controller="at.fhhagenberg.sqe.controller.ElevatorController"
	gridLinesVisible="false">

	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>

	<Button text="Auto" GridPane.columnIndex="0"
		GridPane.rowIndex="0" GridPane.halignment="CENTER" 
		style="-fx-font-size: 12;" >
	</Button>

	<Label text="Elevator Control Center" GridPane.columnIndex="1"
		GridPane.rowIndex="0" GridPane.columnSpan="2" GridPane.halignment="CENTER" 
		style="-fx-font-size: 20;" >
	</Label>

	<GridPane alignment="CENTER" hgap="10.0" vgap="10.0"
		xmlns:fx="http://javafx.com/fxml" GridPane.columnIndex="0"
		GridPane.rowIndex="1">

		<Label text="ElevatorNr" GridPane.columnIndex="0"
			GridPane.rowIndex="1">
		</Label>
		<TextField fx:id="textFieldElevatorNr" GridPane.columnIndex="0"
			GridPane.rowIndex="2" editable="false" >
		</TextField>

		<Label text="Speed" GridPane.columnIndex="0" GridPane.rowIndex="3">
		</Label>
		<TextField fx:id="textFieldSpeed" GridPane.columnIndex="0"
			GridPane.rowIndex="4" editable="false" >
		</TextField>

		<Label text="Acceleration" GridPane.columnIndex="0"
			GridPane.rowIndex="5">
		</Label>
		<TextField fx:id="textFieldAcceleration"
			GridPane.columnIndex="0" GridPane.rowIndex="6" editable="false" >
		</TextField>

		<Label text="Weight" GridPane.columnIndex="0" GridPane.rowIndex="7">
		</Label>
		<TextField fx:id="textFieldWeight" GridPane.columnIndex="0"
			GridPane.rowIndex="8" editable="false">
		</TextField>

		<Label text="Capacity" GridPane.columnIndex="0"
			GridPane.rowIndex="9">
		</Label>
		<TextField fx:id="textFieldCapacity" GridPane.columnIndex="0"
			GridPane.rowIndex="10" editable="false">
		</TextField>

	</GridPane>

	<GridPane fx:id="elevatorsArea" alignment="CENTER" hgap="10.0" vgap="10.0"
		xmlns:fx="http://javafx.com/fxml" GridPane.columnIndex="2"
		GridPane.rowIndex="1" gridLinesVisible="false" >

	</GridPane>
	
</GridPane>




